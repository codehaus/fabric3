<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:f3-core="urn:fabric3.org:core"
           name="PhysicalBootComposite"
           autowire="true">

    <component name="Connector">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.ConnectorImpl"/>
    </component>

    <component name="PhysicalComponentBuilderRegistry">
        <f3-impl:implementation.system class="org.fabric3.fabric.builder.component.DefaultComponentBuilderRegistry"/>
    </component>

    <component name="ExpressionExpander">
        <f3-impl:implementation.system class="org.fabric3.fabric.services.expression.ExpressionExpanderImpl"/>
    </component>

    <!-- add the default evaluators. Host properties override system properties which override environment properties -->
    <component name="HostPropertiesExpressionEvaluator" f3-core:key="100">
        <f3-impl:implementation.system class="org.fabric3.fabric.services.expression.HostPropertiesExpressionEvaluator"/>
    </component>

    <component name="SystemPropertyEvaluator" f3-core:key="101">
        <f3-impl:implementation.system class="org.fabric3.fabric.services.expression.SystemPropertyEvaluator"/>
    </component>

    <component name="EnvironmentPropertyEvaluator" f3-core:key="102">
        <f3-impl:implementation.system class="org.fabric3.fabric.services.expression.EnvironmentPropertyEvaluator"/>
    </component>

</composite>