<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           name="PojoImplementationBootComposite"
           autowire="true">

    <component name="DefaultInstanceFactoryBuilderRegistry">
        <f3-impl:implementation.system class="org.fabric3.pojo.instancefactory.DefaultInstanceFactoryBuilderRegistry"/>
    </component>

    <component name="ReflectiveInstanceFactoryBuilder">
        <f3-impl:implementation.system class="org.fabric3.pojo.reflection.ReflectiveInstanceFactoryBuilder"/>
    </component>

    <component name="GenerationHelperImpl">
        <f3-impl:implementation.system class="org.fabric3.pojo.control.GenerationHelperImpl"/>
    </component>

    <component name="BuildHelperImpl">
        <f3-impl:implementation.system class="org.fabric3.pojo.instancefactory.BuildHelperImpl"/>
    </component>
</composite>
