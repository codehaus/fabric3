    <component name="Property2LongTransformer">
        <f3-impl:implementation.system class="org.fabric3.transform.dom2java.Property2LongTransformer"/>
    </component>

    <component name="Property2FloatTransformer">
        <f3-impl:implementation.system class="org.fabric3.transform.dom2java.Property2FloatTransformer"/>
