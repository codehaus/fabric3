<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:binding"
           name="MetroControllerExtension"
           autowire="true">

    <component name="WsdlResourceProcessor">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.wsdl.WsdlResourceProcessor"/>
    </component>

    <component name="MetroBindingGenerator" f3-core:key="org.fabric3.binding.ws.scdl.WsBindingDefinition">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.control.MetroBindingGenerator"/>
    </component>

    <component name="AddressResolver">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.control.DefaultAddressResolver"/>
    </component>

    <component name="WsdlElementParser">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.control.DefaultWsdlElementParser"/>
    </component>

    <component name="WsdlParser">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.control.DefaultWsdlParser"/>
    </component>

</composite>
