<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:fabric3.org:binding"
           name="MetroControllerExtension"
           autowire="true">

    <component name="WsdlResolver">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.WsdlResolverImpl"/>
    </component>

    <component name="EndpointResolver">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.EndpointResolverImpl"/>
    </component>

    <component name="EndpointSynthesizer">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.EndpointSynthesizerImpl"/>
    </component>

    <component name="WsdlSynthesizer">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.WsdlSynthesizerImpl"/>
    </component>

    <component name="MetroBindingGenerator" f3-core:key="org.fabric3.binding.ws.model.WsBindingDefinition">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.MetroBindingGenerator"/>
    </component>

    <component name="WsdlGeneratorDelegate" f3-core:key="org.fabric3.wsdl.model.WsdlServiceContract">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.WsdlGeneratorDelegate"/>
    </component>

    <component name="JavaGeneratorDelegate" f3-core:key="org.fabric3.spi.model.type.java.JavaServiceContract">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.metro.generator.JavaGeneratorDelegate"/>
    </component>

</composite>
