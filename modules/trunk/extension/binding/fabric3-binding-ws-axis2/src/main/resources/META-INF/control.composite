<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="urn:maven:org.codehaus.fabric3:fabric3-binding-ws-axis2"
           name="Axis2ControllerExtension"
           autowire="true">

    <!-- Ws Binding Generator -->
    <component name="Axis2BindingGenerator" f3-core:key="org.fabric3.binding.ws.scdl.WsBindingDefinition">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.axis2.control.Axis2BindingGenerator"/>
    </component>
    
    <!-- Jaxb Databinding Interceptor Generator -->
    <component name="JaxbInterceptorGenerator" f3-core:key="{urn:fabric3.org:policy}dataBinding.jaxb">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.axis2.control.jaxb.JaxbInterceptorDefinitionGenerator"/>
    </component>
    
    <!--Jaxws Introspector -->
    <component name="JAXWSTypeIntrospector">
        <f3-impl:implementation.system class="org.fabric3.binding.ws.axis2.introspection.JAXWSTypeIntrospector"/>
    </component>

</composite>
