<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           targetNamespace="org.codehaus.fabric3"
           name="WarContributionExtension"
           autowire="true">

    <component name="WarClasspathProcessor">
        <f3-impl:implementation.system class="org.fabric3.contribution.war.WarClasspathProcessor"/>
    </component>

    <component name="WarContributionProcessor">
        <f3-impl:implementation.system class="org.fabric3.contribution.war.WarContributionHandler"/>
    </component>

    <component name="WebXmlIndexer">
        <f3-impl:implementation.system class="org.fabric3.contribution.war.WebXmlIndexer"/>
        <property name="namespace">false</property>
    </component>

    <component name="WebXmlIndexerNamepsaceAware">
        <f3-impl:implementation.system class="org.fabric3.contribution.war.WebXmlIndexer"/>
        <property name="namespace">true</property>
    </component>

</composite>
