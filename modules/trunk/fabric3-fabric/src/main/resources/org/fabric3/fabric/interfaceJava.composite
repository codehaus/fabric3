<!--
    Configuration for Java IDL support.
    
    $Rev$ $Date$
-->
<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           xmlns:system="http://fabric3.org/xmlns/sca/system/2.0-alpha"
           name="org.fabric3.fabric.InterfaceJava"
           autowire="true">

    <component name="interfaceJava.interfaceProcessorRegistry">
        <system:implementation.system class="org.fabric3.fabric.idl.java.JavaInterfaceProcessorRegistryImpl"/>
    </component>
    <component name="interfaceJava.loader">
        <system:implementation.system class="org.fabric3.fabric.idl.java.InterfaceJavaLoader"/>
    </component>
</composite>
