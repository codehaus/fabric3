<composite xmlns="http://www.osoa.org/xmlns/sca/1.0" xmlns:system="http://fabric3.org/xmlns/sca/system/2.0-alpha"
           name="org.fabric3.fabric.ExtensionsDeployment"
           autowire="true">


    <component name="ExtensionContributionProcessor">
        <system:implementation.system
                class="org.fabric3.fabric.services.contribution.processor.ExtensionContributionProcessor"/>
    </component>

    <component name="ExtensionContributionStore">
        <system:implementation.system
                class="org.fabric3.fabric.services.contribution.ContributionStoreImpl"/>
        <property name="storeId">extensions</property>
        <property name="persistent">false</property>
    </component>


</composite>
