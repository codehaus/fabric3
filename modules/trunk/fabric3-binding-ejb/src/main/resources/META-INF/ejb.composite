<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           xmlns:f3="http://fabric3.org/xmlns/sca/2.0-alpha"
           xmlns:system="http://fabric3.org/xmlns/sca/system/2.0-alpha"
           name="EjbTransportBinding">

    <!-- EJB Binding Loader -->
    <component name="EjbBindingLoader">
        <system:implementation.system class="org.fabric3.binding.ejb.model.logical.EjbBindingLoader"/>
    </component>

    <!-- EJB Binding Generator -->
    <component name="EjbBindingGenerator">
        <system:implementation.system class="org.fabric3.binding.ejb.model.physical.EjbBindingGenerator"/>
    </component>

    <!-- EJB Wire Attacher -->
    <component name="EjbWireAttacher">
        <system:implementation.system class="org.fabric3.binding.ejb.wire.EjbWireAttacher"/>
    </component>

    <!-- EJB Link Resolver -->
    <component name="EjbLinkResolver">
        <system:implementation.system class="org.fabric3.binding.ejb.wire.DefaultEjbLinkResolver"/>
    </component>

</composite>
