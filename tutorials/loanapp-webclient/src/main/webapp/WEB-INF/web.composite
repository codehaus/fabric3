<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           targetNamespace="http://fabric3.org/xmlns/sca/tutorials/0.5"
           name="LoanWebAppComposite">

    <component name="lending">
        <implementation.web/>
        <reference name="requestCoordinator" target="RequestCoordinator/RequestCoordinator"/>
        <reference name="acceptanceCoordinator" target="AcceptanceCoordinator/AcceptanceCoordinator"/>
        <reference name="validationService" target="WebValidationComponent"/>
    </component>

    <component name="WebValidationComponent">
        <implementation.composite name="tutorials:ValidationComposite"
                                  scdlResource="loanapp/validation/impl/validation.composite"/>
    </component>

</composite>
