<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           name="NetJAXBComposite">


    <component name="JAXBWeatherTest">
        <f3-impl:junit class="org.fabric3.tests.binding.net.jaxb.JAXBWeatherTest"/>
        <reference name="weatherService">
            <binding.http uri="http://127.0.0.1:8282/JAXBWeatherService"/>
        </reference>
    </component>

    <component name="JAXBWeatherService">
        <implementation.java class="org.fabric3.tests.binding.net.jaxb.WeatherServiceImpl"/>
        <service name="WeatherService">
            <binding.http uri="/JAXBWeatherService"/>
        </service>
    </component>


</composite>
