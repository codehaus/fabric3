<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200903"
           xmlns:f3-core="urn:fabric3.org:core"
           xmlns:f3-impl="urn:fabric3.org:implementation"
           xmlns:f3-binding="urn:fabric3.org:binding"
           xmlns:weather="urn:weather"
           targetNamespace="org.codehaus.fabric3"
           name="WsdlTestComposite">

    <component name="FromWSDLTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.generatedFromWsdl.TestClient"/>
        <reference name="service" target="FromWsdlWeatherService"/>
    </component>

    <component name="FromWsdlWeatherService">
        <f3-impl:implementation.wsdl service="weather:WeatherPortType" stub="org.fabric3.tests.wsdl.generatedFromWsdl.WeatherStub"/>
    </component>


    <component name="XmlRootElementTestClient">
        <f3-impl:junit class="org.fabric3.tests.wsdl.xmlrootelement.TestClient"/>
        <reference name="service" target="XmlRootElementWeatherService"/>
    </component>

    <component name="XmlRootElementWeatherService">
        <f3-impl:implementation.wsdl service="weather:WeatherPortType" stub="org.fabric3.tests.wsdl.xmlrootelement.WeatherStub"/>
    </component>

</composite>
