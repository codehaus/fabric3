<config>
    <runtime mode="participant" jmx.port="7100-7500"/>
    <web.server>
        <http port="8100-8200"/>
    </web.server>
    <federation>
        <transport>
            <value>
                <entry><key>http</key><value>localhost:8181</value></entry>
                <entry><key>binding.net.http</key><value>localhost:8282</value></entry>
            </value>
        </transport>
    </federation>
</config>
